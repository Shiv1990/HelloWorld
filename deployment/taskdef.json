{
  "family": "sampleapp-taskdef",
  "networkMode": "bridge",
  "memory": "300",
  "containerDefinitions": [
      {
          "image": "729769420771.dkr.ecr.us-east-1.amazonaws.com/sampleapp:%BUILD_NUMBER%",
          "name": "sampleapp",
          "cpu": 0,
          "memory": 256,
          "essential": true,
          "portMappings": [
              {
                  "containerPort": 9009,
                  "hostPort": 0,
                  "protocol": "tcp"
              }
          ]
      }
  ],
  "requiresCompatibilities": [
    "EC2"
]
}